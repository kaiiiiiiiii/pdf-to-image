{
  "name": "pdf-to-image",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://github.com/kaiiiiiiiii/pdf-to-image",
  "type": "module",
  "scripts": {
    "dev": "bun run update-config && vite dev --port 3000",
    "start": "node .output/server/index.mjs",
    "build": "bun run update-config && vite build",
    "test": "vitest run",
    "test:e2e": "playwright test",
    "playwright:install": "playwright install",
    "lint": "oxlint",
    "format": "oxfmt --check .",
    "check": "oxfmt --write . && oxlint --fix",
    "check-unused": "depcheck",
    "update-config": "node scripts/update-config.js"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/vite": "^4.1.13",
    "@tanstack/react-devtools": "^0.2.2",
    "@tanstack/react-router": "~1.131.41",
    "@tanstack/react-router-devtools": "~1.131.42",
    "@tanstack/react-router-ssr-query": "~1.131.41",
    "@tanstack/react-start": "~1.131.41",
    "@tanstack/router-plugin": "~1.131.41",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jszip": "^3.10.1",
    "lucide-react": "^0.476.0",
    "pdfjs-dist": "^5.4.149",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13",
    "tw-animate-css": "^1.3.8",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^3.25.76"
